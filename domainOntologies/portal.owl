  <!ENTITY portal.owl "http://127.0.0.1:8000/ontology/portal.owl">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY support.owl "http://127.0.0.1:8000/ontology/support.owl">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="&portal.owl;"
         xmlns:owl="&owl;"
         xmlns:portal="&portal;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
		 xmlns="&portal;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.0">
    <rdfs:label>AKT Reference Ontology (Portal Ontology)</rdfs:label>
    <rdfs:seeAlso rdf:resource="http://d3e.open.ac.uk/akt/2002/ref-onto.html"/>
    <owl:imports>
      <owl:Ontology rdf:about="&support.owl;"/>
    </owl:imports>
  </owl:Ontology>

<!-- Classes -->
  <owl:Class rdf:about="#Abstract-Information">
    <rdfs:comment>Information in general, independent of an object in which it is encoded. Whatever is transferred in an information-transfer event. It is clearly an intangible thing</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="&support.owl;#Intangible-Thing"/>
  </owl:Class>

  <owl:Class rdf:about="#Academic">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Educational-Employee"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Higher-Educational-Organization"/>
        <owl:onProperty rdf:resource="#works-for"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Academic-Degree">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Degree"/>
  </owl:Class>

  <owl:Class rdf:about="#Academic-Item-Number">
    <rdfs:subClassOf rdf:resource="#Publication-Number"/>
  </owl:Class>

  <owl:Class rdf:about="#Academic-Support-Staff">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Educational-Employee"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Higher-Educational-Organization"/>
        <owl:onProperty rdf:resource="#works-for"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Academic-Support-Unit">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Educational-Organization-Unit"/>
  </owl:Class>

  <owl:Class rdf:about="#Academic-Unit">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Educational-Organization-Unit"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#University"/>
        <owl:onProperty rdf:resource="#unit-of-organization"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Activity">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="&support.owl;#Temporal-Thing"/>
  </owl:Class>

  <owl:Class rdf:about="#Address">
    <rdfs:comment>A generic class for addresses, whether email or postal. We see an address as abstract information and therefore it is an intangible thing</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Abstract-Information"/>
  </owl:Class>

  <owl:Class rdf:about="#Affiliated-Person">
    <rdfs:comment>A person which has an affiliation with some organization. For instance employees are affiliated to the organization they work for, students to the institution where they are studying, etc. A person can have multiple affiliations, which means that there is no constraint relating the values of slot has-affiliation-to-unit to the values of slot has-affiliation</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Person"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
        <owl:onProperty rdf:resource="#has-affiliation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Amount-Of-Money">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="&support.owl;#Physical-Quantity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Currency"/>
        <owl:onProperty rdf:resource="&support.owl;#has-unit-of-measure"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Appellation">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="&support.owl;#Intangible-Thing"/>
  </owl:Class>

  <owl:Class rdf:about="#Article-In-A-Composite-Publication">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Publication"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Composite-Publication"/>
        <owl:onProperty rdf:resource="#included-in-publication"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Article-Reference">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Publication-Reference"/>
  </owl:Class>

  <owl:Class rdf:about="#Attending-A-Conference">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Attending-An-Event"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Conference"/>
        <owl:onProperty rdf:resource="#event-attended"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Attending-An-Event">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Event"/>
  </owl:Class>

  <owl:Class rdf:about="#Award">
    <rdfs:comment>An award is an intangible thing, even if the piece of paper which is often associated with an award is tangible.  What about the virtual piece of paper in the virtual degree ceremony?  I guess that ought to be an intangible</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="&support.owl;#Intangible-Thing"/>
  </owl:Class>

  <owl:Class rdf:about="#Awarding-Body">
    <rdfs:comment>Legal agents can be either organizations or people. An awarding body is normally an organization, an individual, or a bunch of people</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Legal-Agent"/>
  </owl:Class>

  <owl:Class rdf:about="#Book">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Publication"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Book-Reference"/>
        <owl:onProperty rdf:resource="#has-publication-reference"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
        <owl:onProperty rdf:resource="#has-publication-reference"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Book-Publishing">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Publication-Type-Event"/>
  </owl:Class>

  <owl:Class rdf:about="#Book-Reference">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Publication-Reference"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Publishing-House"/>
        <owl:onProperty rdf:resource="#published-by"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&xsd;string"/>
        <owl:onProperty rdf:resource="#has-ISBN-number"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Book-Section-Reference">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Publication-Reference"/>
  </owl:Class>

  <owl:Class rdf:about="#Business-Area">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Generic-Area-Of-Interest"/>
  </owl:Class>

  <owl:Class rdf:about="#Capital-City">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#City"/>
  </owl:Class>

  <owl:Class rdf:about="#Charitable-Organization">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Non-Profit-Organization"/>
  </owl:Class>

  <owl:Class rdf:about="#City">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Municipal-Unit"/>
  </owl:Class>

  <owl:Class rdf:about="#Civil-Service">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Government-Organization"/>
  </owl:Class>

  <owl:Class rdf:about="#Company">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Profit-Organization"/>
  </owl:Class>

  <owl:Class rdf:about="#Composite-Publication">
    <rdfs:comment>A publication which contains items which cane be themselves referenced through a publication reference.  Composite publications include newspapers, magazines and journals. A book which is a collection of articles is a composite publication, a monograph is not</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Publication"/>
        <owl:onProperty rdf:resource="#contains-publication"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
        <owl:onProperty rdf:resource="#contains-publication"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Computing-Technology">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Technology"/>
  </owl:Class>

  <owl:Class rdf:about="#Conference">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Meeting-Taking-Place"/>
    <rdfs:subClassOf rdf:resource="#Publication-Type-Event"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Conference-Proceedings-Reference"/>
        <owl:onProperty rdf:resource="#published-proceedings"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Conference-Proceedings-Reference">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Publication-Reference"/>
  </owl:Class>

  <owl:Class rdf:about="#Conferring-An-Award">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Generalized-Transfer"/>
  </owl:Class>

  <owl:Class rdf:about="#Country">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Geopolitical-Entity"/>
  </owl:Class>

  <owl:Class rdf:about="#Currency">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Information-Bearing-Object"/>
  </owl:Class>

  <owl:Class rdf:about="#DIN-A4"
             rdfs:label="DIN-A4 page size">
    <rdfs:subClassOf rdf:resource="#Page-Size"/>
  </owl:Class>

  <owl:Class rdf:about="#Daily-Newspaper">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Newspaper"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&support.owl;#twentyfour-hour-duration"/>
        <owl:onProperty rdf:resource="#has-periodicity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Degree"
             rdfs:comment="A degree is type of award">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Award"/>
  </owl:Class>

  <owl:Class rdf:about="#Distance-Teaching-University">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#University"/>
  </owl:Class>

  <owl:Class rdf:about="#Edited-Book">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Publication"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
        <owl:onProperty rdf:resource="#has-publication-reference"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Edited-Book-Reference"/>
        <owl:onProperty rdf:resource="#has-publication-reference"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Edited-Book-Reference">
    <rdfs:isDefinedBy rdf:resource="http://127.0.0.1:8000/ontology/"/>
    <rdfs:subClassOf rdf:resource="#Book-Reference"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Person"/>
        <owl:onProperty rdf:resource="#edited-by"/>
      </owl:Restriction>
    </rdfs:subClassOf>
